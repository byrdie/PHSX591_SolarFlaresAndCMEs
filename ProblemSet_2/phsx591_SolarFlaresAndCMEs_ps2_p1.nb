Cell["The flux function for this problem is given by", "Subsubsection",
 CellChangeTimes->{{3.6973031927977934`*^9, 3.697303202631012*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"A", " ", "=", " ", 
   RowBox[{
    RowBox[{"\[Lambda]", " ", 
     RowBox[{"ArcTan", "[", 
      FractionBox[
       RowBox[{"y", "+", "b"}], "z"], "]"}]}], "-", 
    RowBox[{"\[Lambda]", " ", 
     RowBox[{"ArcTan", "[", 
      FractionBox[
       RowBox[{"y", "-", "b"}], "z"], "]"}]}], " ", "+", " ", 
    RowBox[{
     FractionBox["Ics", "c"], 
     RowBox[{"Log", "[", 
      FractionBox[
       RowBox[{
        SuperscriptBox["y", "2"], " ", "+", " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"z", "+", "h"}], ")"}], "2"]}], 
       RowBox[{
        SuperscriptBox["y", "2"], "+", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"z", "-", "h"}], ")"}], "2"]}]], "]"}]}]}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.6973032073053503`*^9, 3.6973032477567496`*^9}, {
  3.6973032928801303`*^9, 3.6973032973204107`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["We can then find the magnetic field by taking a curl", "Subsubsection",
 CellChangeTimes->{{3.697303334577202*^9, 3.6973033440540776`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"B", " ", "=", " ", 
  RowBox[{
   RowBox[{"Cross", "[", 
    RowBox[{
     RowBox[{"Grad", "[", 
      RowBox[{"A", ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y", ",", "z"}], "}"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", "0", ",", "0"}], "}"}]}], "]"}], " ", "//", 
   "FullSimplify"}]}]], "Input",
 CellChangeTimes->{{3.697303353854903*^9, 3.69730335955628*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"0", ",", 
   RowBox[{
    FractionBox[
     RowBox[{"4", " ", "h", " ", "Ics", " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["h", "2"], "+", 
        SuperscriptBox["y", "2"], "-", 
        SuperscriptBox["z", "2"]}], ")"}]}], 
     RowBox[{"c", " ", 
